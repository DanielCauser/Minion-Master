<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="MinionMaster.Root.MainPage" Title="{Binding Title}">
    <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
        <Button Text="Master" HeightRequest="200" WidthRequest="400" HorizontalOptions="FillAndExpand" Command="{Binding GoToMaster}"/>
        <Button Text="Minion" HeightRequest="200" WidthRequest="400"  HorizontalOptions="FillAndExpand" Command="{Binding GoToMinion}"/>
    </StackLayout>
</ContentPage>